{
  "name": "itgmanager",
  "productName": "ITGManager",
  "version": "1.0.0",
  "description": "OpenITG/NotITG Songs Manager",
  "license": "ISC",
  "author": "SaitoxBeats",
  "type": "commonjs",
  "main": "src/index.js",
  "scripts": {
    "start": "electron .",
    "package": "electron-packager . ITGManager --platform=win32 --arch=x64 --icon=src/img/icon.ico --out=dist"
  },
  "devDependencies": {
    "electron": "^35.2.1"
  },
  "dependencies": {
    "adm-zip": "^0.5.10",
    "electron-packager": "^17.1.2",
    "electron-store": "^8.1.0"
  },
  "build": {
    "appId": "com.SaitoxBeats.ITGManager",
    "files": [
      "src/**/*",
      "package.json"
    ],
    "win": {
      "icon": "src/img/icon.ico"
    }
  }
}
